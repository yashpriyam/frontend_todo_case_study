{
  "name": "trello-clone",
  "version": "1.0.0",
  "description": "Trello Clone",
  "main": "app.js",
  "author": "Yash Priyam <yashpriyam74@gmail.com>",
  "license": "ISC",
  "scripts": {
    "build-fe": "cd react-client && npm run build",
    "react-client-install": "cd react-client && npm install",
    "heroku-postbuild": "npm run react-client-install && npm run build-fe && npm run build",
    "local-dev": "nodemon node-server/app.js",
    "dev": "nodemon --exec npm run restart",
    "restart": "rimraf dist && npm run build && npm run start",
    "start": "cross-env ENV_NAME=dev npm-run-all --parallel start:server start:app",
    "start:app": "react-scripts start",
    "start:server": "netlify-lambda serve node-server -t 900",
    "build": "npm-run-all --parallel build:**",
    "build:app": "react-scripts build",
    "debug": "netlify dev --inspect",
    "build:functions": "netlify-lambda build node-server",
    "test": "react-scripts test --env=jsdom"
  },
  
  "dependencies": {
    "axios": "^0.24.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie": "^0.4.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "mongodb": "^3.6.9",
    "mongoose": "^6.0.14",
    "react": "^17.0.1",
    "react-avatar-editor": "^12.0.0",
    "react-dom": "^17.0.1",
    "react-router": "^6.2.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.1",
    "react-toastify": "^7.0.4",
    "serverless-http": "^2.3.0",
    "snappy": "^7.1.1",
    "socket.io": "^4.2.0",
    "socket.io-client": "^4.2.0",
    "validator": "^10.9.0",
    "vue": "^2.5.17",
    "vue-server-renderer": "^2.5.17",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^0.4.1",
    "netlify-lambda": "^1.6.3",
    "nodemon": "^2.0.14",
    "npm-run-all": "^4.1.3",
    "parcel-bundler": "^1.9.4",
    "rimraf": "^2.6.2",
    "webpack": "4.44.2",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.2.1",
    "webpack-dev-server": "3.11.0"
  },
  "proxy": "http://localhost:9000/.netlify/functions/app/",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
